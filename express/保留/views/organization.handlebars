<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" type="text/css" href="http://{{host}}:{{port}}/stylesheets/organization.css">
    <script src="http://{{host}}:{{port}}/javascripts/organization.js" type="text/javascript" charset="utf-8" ></script>
    <title>Class Manger</title>
</head>
<body>
	<div class="members">
		members
		<ul>
			{{#each organization_data.members}}
		  		<li>name:{{name}}, student_id:{{student_id}}, position:{{position}}</li>
		  	{{/each}}
		</ul>
	</div>
	<div class="homeworks">
		homeworks
		<a href="http://{{host}}:{{port}}/create/homework">create_homework</a>
		<ul>
			{{#each organization_data.homeworks}}
		  		<li><a href="http://{{../host}}:{{../port}}/organizations/{{account}}">edit</a>name:{{name}}, content:{{content}}, deadline:{{deadline}}</li>
		  	{{/each}}
		</ul>
	</div>
	<div class="notices">
		notices
		<a href="http://{{host}}:{{port}}/create/notice">create_notice</a>
		<ul>
			{{#each organization_data.notices}}
		  		<li><a href="http://{{../host}}:{{../port}}/organizations/{{account}}">edit</a>name:{{name}}, content:{{content}}, deadline:{{deadline}}</li>
		  	{{/each}}
		</ul>
	</div>
	<div class="votes">
		votes
		<a href="http://{{host}}:{{port}}/create/vote">create_vote</a>
		<ul>
			{{#each organization_data.votes}}
		  		<li><a href="http://{{../host}}:{{../	port}}/organizations/{{account}}">edit</a>name:{{name}}, content:{{content}}, deadline:{{deadline}}</li>
		  	{{/each}}
		</ul>
	</div>
</body>
</html>